/* Footer */

#footer {
	width: 980px;
	float:left;
	margin: 0px 0px 15px 0px;
	border-top: 1px solid #E5E5E5;
	color: #757E82;
	background:#f5f5f5;
	border-bottom:3px double #86aeff;
	border-right:3px double #86aeff;
	border-left:3px double #86aeff;
}

#footer p {
	margin: 0;
	font-size: 77%;
	padding:6px 10px;
	text-align:center;
}

#footer p img{
	border: none;
}

#legal{
	float:left;
	text-align: left;
}

#links{
	float:right;
	text-align: right;
}

#footer a {
	text-decoration: underline;
	color: #757E82;
}

#footer a:hover {
	text-decoration: none;
}


feedback a {
background:none repeat scroll 0 0 #FEF9CD;
border-bottom:2px double #6495ED;
border-right:2px double #6495ED;
border-top:2px double #6495ED;
display:block;
left:-1px;
position:fixed;
top:200px;
}


#feedback img {
	border:none;
}

#feedback a  {
	background:#FEF9CD;
	border-top:2px double #6495ED;
	border-right:2px double #6495ED;
	border-bottom:2px double #6495ED;
	display:block;
	position:fixed;
	left:-1px;
	top:200px;
}
#feedback a:hover {
	border-color:#0001aa;
	background:#e0ecf8;
}

#latest{
	float: left;
	width:100%;
	padding:15px 0px;
}


.lrss{
	float: right;
	padding-right:15px;
}

.lrss img{
	vertical-align: middle;
}

#latest a {
	text-decoration:none;
}

#latest a:hover{
	text-decoration:underline;
}

#latest li {
	padding:4px 6px;
	border-bottom: 1px dotted #dddddd;
	float:left;
	width:632px;
}

.toggler{
	color:#335577;
	cursor: pointer;
}

.togglered{
	display: none;
}

#latest .datainfo{
float:right;
font-size:90%;
width:90%;

}

#latest .datainfo .lefterer{
	width:80%;
	float: left;
}

#latest .datainfo .datainfodate{
	width:30%;
	float: left;
	/*font-weight:bold; */
}

#latest .datainfo .datainfoinst{
	width:70%;
	float: left;
}

#latest .datainfo .datainfmore{
	float: right;
	width:15%;
	font-weight:bold;
	text-align: right;
}

.mininfo{
float:left;
font-size:0.75em;
padding:10px 0 5px;
width:100%;
}

.subtitle{
	color:#781D1D;
	font-size:1.1em;
	font-weight:bold;
	margin:0;
	padding:0 !important;
}

.gohome{
	float:left;
	font-size:1.05em;
	text-decoration:none;
	font-weight:bold;
	color:#1248a9;
	padding:0 0 10px 10px;
}
