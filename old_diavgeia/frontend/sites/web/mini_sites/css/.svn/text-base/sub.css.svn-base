/* Sidebar */

#sidebar {
	float: right;
	width: 248px;
	padding-top: 30px;
}

#sidebar ul {
	margin: 0;
	padding: 0;
	list-style: none;
	line-height: normal;
}

#sidebar li {
	margin-bottom: 1px;
}

#sidebar li ul {
	float:left;
	margin:0;
	padding:8px 0 20px;
	width:100%;
}

#sidebar ul.help li {
	background-image:url("images/help.gif");
	background-position: left;
	background-repeat: no-repeat;
	padding-left:20px;
	text-align:left;
}

#sidebar li li {
	margin: 0;
	padding: 6px 0px;
	border-bottom: 1px solid #EBEBEB;
	width:100%;
	float:left;
	text-align:center;
}

#sidebar h2 {
	margin: 0px;
	padding: 4px 5px;
	border-bottom: 2px solid #6495ed;
	font: normal 16px Georgia, "Times New Roman", Times, serif;
	color: #454E55;
	background: #EBEBEB;
	float: left;
	text-align: center;
}

#sidebar h3 {
	font-size: 77%;
	color: #454E55;
}

#sidebar p {
	margin: 0;
	line-height: normal;
	color: #D4C792;
}

#sidebar a {
	border: none;
	text-decoration: none;
}

#sidebar a:hover {
	text-decoration: underline;
}

#sidebar a.more-link{
	float:left;
	width:100%;
	text-align: right;
	padding: 4px 0px;
	border-top: 2px solid #6495ed;
	font: bold 12px Arial, "Times New Roman", Times, serif;
	font-style: italic;
	color: #454E55;
}

a.search_archive{
	float: left;
	text-align:left;
	background-image:url("images/search.png");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 45px;
	border:1px solid #cacaca !important;
	margin:0px 0px 20px 0px;
	cursor: pointer;
	min-width:80%;
}

a.search_archive:hover{
	color:#003399;
	border:1px solid #003399 !important;
	text-decoration: none !important;
}

a.perifereia{
	/*float: right;*/
	text-align:left;
	background-color:#ffffbf;
	padding:10px 15px;;
	border:1px solid #cacaca !important;
}


#info1 {
	float:left;
	padding:5% 10%;
	width:80%;
	text-align: center;
}

#info1 a {
background-color: #FFFF99;
font-size:14px;

}

