* {
	margin: 0;
	padding: 0;
}


img {
border:none;

}

body {
	background-color:#e1e1e1;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #666666;
}

h1, h3 {
	color:#666666;
}

h4 {
width:600px;
font-size:110%;
}

 h2 {
	color:#666666;
	border-bottom-width: 2px;
	border-bottom-style: solid;
	border-bottom-color: #333333;
	padding:10px;
	font-size:14px;
}


h1 {
}

h2 {
}

h3 {
}

p, blockquote, ul, ol {
	margin-bottom: 20px;
	line-height: 2em;
}

p {
}

blockquote {
}

ul, ol, li {
	margin: 0px;
	padding: 0px;
	list-style: none;
}

a {
	text-decoration: underline;
	color: #335577;
}

a:hover {
	text-decoration: none;
	color: #335577;
}

/* Wrapper */

#wrapper {
	margin: 0 auto;
	width:980px;

}

/* Header */

#header {
	width: 980px;
	float:left;
	background: #ffffff;
	margin:15px 0px 0px 0px;
	border-bottom: 2px solid #EBEBEB;
	border-top:3px double #86aeff;
	border-right:3px double #86aeff;
	border-left:3px double #86aeff;
}




/* Logo */

#logo {
	float: left;
	width: 440px;
	margin: 0px;
	padding: 35px 0px 30px 30px;
}

#logo img{
	float: left;
	padding-right:10px;
}

#logo h1 {
	margin: 0;
	padding: 0;
	font: normal 19px Georgia, "Times New Roman", Times, serif;
}

#logo h2 {
	margin: 0;
	padding: 6px 0px;
	font-size: 11px;
	color: #444444;
}

#logo h2 a {
	color: #9AA9B1;
}

#logo a {
	text-decoration: none;
	color: #3C84B3;
}

/* Menu */

#menu {
	float: right;
	width: 510px;
	margin: 20px 0px;
}

#menu ul {
	margin: 0;
	padding: 0px 0 0 20px;
	list-style: none;
	line-height: normal;
	float: right;
}

#menu li {
	display: inline;
	text-align: center;
}

#menu a {
	display: block;
	float: left;
	padding: 0px 10px;
	text-decoration: none;
	text-align: center;
	font-family: Arial, Helvetica, sans-serif;
	color: #464E53;
}

#menu a:hover, #menu .active a {
	color: #335577;
	text-decoration: underline;
}

#page {
	width: 980px;
	float:left;
	background: #ffffff;
	border-right:3px double #86aeff;
	border-left:3px double #86aeff;
	padding-bottom:10px;
}

#page-bgtop {
	padding: 0px 20px;
}

/* Content */

#content {
	float: left;
	width: 644px;
	padding-top: 30px;
}

#notice{
	padding:15px;
	background:#fefce6;
	border: 1px solid #cacaca;
	font-size:110%;
	float: left;
	width: 610px;
}
#notice p{
	line-height:1.2em;
	text-align: left;
}

#notice .morenotice{
	float:right;
}

#notice .morenotice a{
	font-weight: bold;
	text-decoration: none;
}

#latest_docs{
	font: normal 18px Georgia, "Times New Roman", Times, serif;
	color:#003399;
	float:left;
	width:100%;
	padding:8px 0px;
	margin: 15px 0px;
	border-bottom:2px solid #cacaca;
}

#content .doc{
	float:left;
	width:100%;
	background: #f5f6fc;
	border-bottom:1px solid #cacaca;
	padding:0;
	margin:8px 0px;
	text-align: left;
}

#content .doctitle{
	font: normal 14px Georgia, "Times New Roman", Times, serif;
	width:628px;
	border-bottom: 1px dotted #cacaca;
	padding:10px 8px;
}

#content .doctitle a{
	color: #303f9e;
	text-decoration:none;
}

#content .doctitle a:hover{
	color:#ff8c00;
}

#content .docinfo{
	font-size:11px;
	float: left;
	width:382px;
	padding:4px;	
}

#content .docinfo span{
	float: left;
	width:374px;
	padding:4px;
}

#content span.date{
	
}

#content span.ada{
	
}

#content .doctype{
	font-size:11px;
	float: left;
	width:236px;
	padding:4px 14px 4px 4px;	
}

#content .doctype span{
	float: left;
	width:228px;
	padding:4px;	
	text-align:right;
}

#content .doctype a.getdoc{
	float: right;
	text-align:right;
	background-image:url("images/pdf.gif");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 20px;
	text-decoration: none;
}
#content .doctype a.getdoc:hover{
	float: right;
	text-align:right;
	background-image:url("images/get.png");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 20px;
	text-decoration: undeline;
}

a.moredocs{
	float:right;
	margin:10px 10px 20px 0px;
	padding: 8px;
	border: 1px solid #cacaca;
	text-decoration:none;
}

a.moredocsleft{
	float:left !important;
}

a.moredocs:hover{
	border: 1px solid #204fd7;
	color:#204fd7;
	background:#e9e9e9;
}

#doc_types{
	color:#767676;
}

.bytype{
	float:left;
	width:644px;
}

.per_type{
	width:246px;
	float:left;
	padding:0px 2px;
}

.rss_feed{
	width:386px;
	float:left;
	padding:0px 2px;
}

.doc_type_title{
	color:#767676;
}

.doc_type_title img{
	border:none;
	vertical-align: middle;
}

ul.per_type_list{
	float: left;
	width:226px;
	padding: 10px;
	background:#eff6fc;
}

ul.per_type_list li{
	padding:2px 0px;
	border-bottom:1px dotted #cacaca;
}

ul.per_type_list li a{
	text-decoration:none;
}

ul.per_type_list li a span{
	font-weight: bold;
	font-size:11px;
}

ul.per_type_list li a:hover{
	text-decoration:underline;
}

ul.feed_list{
	float: left;
	width:366px;
	padding: 0px 10px;
}

ul.feed_list li{
	padding:2px 0px;
	border-bottom:1px dotted #cacaca;
}
	
ul.feed_list li a{
	text-decoration:none;
}

ul.feed_list li:hover{
	background:#eff6fc;
}
