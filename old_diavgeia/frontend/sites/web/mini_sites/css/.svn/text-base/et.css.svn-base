}

#feedback img {
	border:none;
}

#feedback a  {
	background:#FEF9CD;
	border-top:2px double #6495ED;
	border-right:2px double #6495ED;
	border-bottom:2px double #6495ED;
	display:block;
	position:fixed;
	left:-1px;
	top:200px;
}
#feedback a:hover {
	border-color:#0001aa;
	background:#e0ecf8;
}

