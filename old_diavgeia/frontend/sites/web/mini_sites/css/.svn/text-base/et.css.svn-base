/* CSS3 ROUNDED BOX */
.rounded_box {
    margin-left: 15px; 

    height: 30px; 
    background-color: #b3c1de; /* #335577; */
    box-shadow: 7px 7px 2px rgb(233, 233, 233); 
    -moz-box-shadow: 7px 7px 2px rgb(233, 233, 233);
    -webkit-box-shadow: 7px 7px 2px rgb(233, 233, 233);
    padding: 5px 10px 8px; 
    width: 250px;
    height: 60px;
    padding: 10px;
    text-align: center;
}

.rounded_box a {
    color: white; 
     text-decoration: none;
     font-weight: bold;
     font-family: Verdana;
}

.rounded_box a:hover {
    text-decoration: underline;
}

