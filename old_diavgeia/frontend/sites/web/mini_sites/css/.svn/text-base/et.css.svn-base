* {
	margin: 0;
	padding: 0;
}

body {

	background-color:#014F8E;
	text-align: left;
	font-family: Arial,Verdana, Helvetica, sans-serif;
	font-size: 13px;
	color: #666666;
}

h1, h2, h3 {
	color: #014F8E;
}

h1 {
}

h2 {
}

h3 {
}

p, blockquote, ul, ol {
	margin-bottom: 20px;
	line-height: 2em;
}

p {
}

blockquote {
}

ul, ol, li {
	margin: 0px;
	padding: 0px;
	list-style: none;
}

a {
	text-decoration: underline;
	color: #DB6A13;
}

a:hover {
	text-decoration: none;
	color: #335577;
}

/* Wrapper */

#wrapper {
	margin: 0 auto;
	width:1000px;
	background-color: #014F8E;
}

/* Header */

#header {
	width: 1000px;
	float:left;
	background: #ffffff;
	margin:10px 0px 0px 0px;
}



/* Logo */

#logo {
	float: left;
	margin: 0px;
	padding: 10px 0px 20px 20px;
}

#logo img{
	float: left;
	padding-right:10px;
}

#logo h1 {
	margin: 0;
	padding: 0;
}

#logo h2 {
	margin: 0;
	padding: 6px 0px;
	font-size: 11px;
	color: #444444;
}

#logo h2 a {
	color: #9AA9B1;
}

#logo a {
	text-decoration: none;
	color: #3C84B3;
}

/* Menu */

#menu {
	float: left;
	width: 100%;
	margin: 20px 0px 10px 0px;
}

#menu ul {
	margin: 0;
	padding: 0px 0 0 20px;
	list-style: none;
	line-height: normal;
	float: right;
}

#menu li {
	display: inline;
	text-align: center;
}

#menu a {
	display: block;
	float: left;
	padding: 0px 10px;
	text-decoration: none;
	text-align: center;
	font-family: Arial, Helvetica, sans-serif;
	color: #464E53;
}

#menu a:hover, #menu .active a {
	color: #335577;
	text-decoration: underline;
}

#page {
	width: 1000px;
	float:left;
	background: #ffffff;

	padding-bottom:10px;
}



#page-bgtop {
	padding: 0px 20px;
}

/* Content */

#content {
	float: left;
	width: 610px;
	padding: 20px 0px;
}

#notice{
	padding:15px;
	background:#fef9cd;
	border: 1px solid #cacaca;

}
#notice p{
	line-height:1.2em;
	text-align: left;
}

.read_on{
	font-weight:bold;
	text-align:right;
}

.signer,
.archive_title,
.archive_info{
	float:left;
	width:100%;
}

.signer,
.archive_title{
	padding:10px 0px 10px 0px;
	font-weight: normal;
}

.archive_title span,
.signer span{
	font:14px Georgia,"Times New Roman",Times,serif;
	font-weight:bold;
	float: right;
	width:90%;
	padding:8px 0px 0px 0px;
	color:#303F9E;
}

.archive_info{
	text-align: right;
	border-top:2px solid #cacaca;
	padding:8px 0px 15px 0px;
	font-style: italic;
	font-size:90%;
}

.latest_docs{
font-size: 18px;
	color:#003399;
	float:left;
	width:100%;
	padding:10px;
	margin: 15px 0px;
	background-color: #F2F2F2;

}

.latest_docs span.title{
	float: left;
}

.latest_docs span.rss{
	float: right;
	font-size: 80%;
}

.latest_docs span.rss a{
	color:#ff8c00;
	text-decoration: none;
}

.latest_docs span.rss img{
	border: none;
	vertical-align: middle;
}

#content .doc{
	float:left;
	width:100%;

	padding:0;
	margin:8px 0px;
	text-align: left;
}

#content .doctitle{
	font: normal 14px Georgia, "Times New Roman", Times, serif;
	border-bottom: 1px dotted #cacaca;
	padding:10px 8px;
}

#content .doctitle a{
	color: #303f9e;
	text-decoration:none;
	font-size: 17px
}

#content .doctitle a:hover{
	color:#ff8c00;
}

#content .docinfo{
	font-size:11px;
	float: left;
	width:462px;
	padding:4px;	
}

#content .docinfo span{
	float: left;
	padding:4px;
}

#content .foreasinfo{
	font-size:11px;
	float: left;
	padding:4px;	
}

#content .foreasinfo span{
	float: left;
	padding:4px;
}

#content span.date{
	float: left;
	width:234px; 	
}

#content span.ada{
	
}

#content span.type{

}

#content .doctype{
	font-size:11px;
	float: left;
	width:452px;
	padding:4px 14px 4px 4px;	
}

#content .doctype span{
	float: left;
	padding:4px;	
	
}

#content .doctype a.getdoc{
	float: right;
	text-align:right;
	background-image:url("../images/pdf.gif");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 20px;
	text-decoration: none;
}
#content .doctype a.getdoc:hover{
	float: right;
	text-align:right;
	background-image:url("images/get.png");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 20px;
	text-decoration: underline;
}

.paginator{
	float:left;
	width:100%;
}

a.moredocs{
	float:right;
	margin:10px 10px 20px 0px;
	padding: 8px;
	border: 1px solid #cacaca;
	text-decoration:none;
}

a.moredocsleft{
	float:left !important;
}

a.moredocs:hover{
	border: 1px solid #204fd7;
	color:#204fd7;
	background:#e9e9e9;
}

#doc_types{
	color:#767676;
}

.bytype{
	float:left;
	width:940px;
}

.per_type{
	width:303px;
	float:left;
	padding:0px 5px;
}

.doc_type_title{
	color:#767676;
}

ul.per_type_list{
	float: left;
	width:583px;
	padding: 20px;
	margin: 10px;
}

ul.per_type_list li{
	padding:2px 0px;
	border-bottom:1px dotted #cacaca;
}

ul.per_type_list li a{
	text-decoration:none;
}

ul.per_type_list li a span{
	font-weight: bold;
	font-size:11px;
}

ul.per_type_list li a:hover{
	text-decoration:underline;
}

.breadcrumb{
	float:left;
	width:100%;
	padding:10px 0px 20px 0px;
}

#content .single_doc a.getdoc{
	float: left;
	background-image:url("../images/pdf.gif");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 20px;
	text-decoration: none;
}
#content .single_doc a.getdoc:hover{
	float: left;
	background-image:url("images/get.png");
	background-position: left;
	background-repeat: no-repeat;
	padding:4px 0 4px 20px;
	text-decoration: underline;
}

#content .single_doc ul{
	float: left;
	width:100%;
}

#content .single_doc ul li{
	float: left;
	width:100%;
	border-top:1px dotted #cacaca;
	padding-top:5px;
}

.single_doc{
	float:left;
	padding:10px 0px;
}

#content .single_doc ul li span{
	float: left;
	width:25%;
	font-weight: bold;
	padding:0px 0px 15px 0px;
	color:black;
}

#content .single_doc ul li span.hash {
	width: 75%;
	float: right;
	font-weight: normal;
	font-style: normal;
	font-size: 80%;
	color: black;
}

#content .single_doc ul li ul{
	width:75%;
	float: right;
}

#content .single_doc ul li ul li{
	float: left;
	border-top:none;
	border-bottom:1px dotted #e9e9e9;
}

.searchpage{
	width:100% !important;
	padding-bottom:20px;
}

/* Sidebar */

#sidebar {
	float: right;
	width: 248px;
	padding-top: 30px;
}

#sidebar ul {
	margin: 0;
	padding: 0;
	list-style: none;
	line-height: normal;
}

#sidebar li {
	margin-bottom: 1px;
}

#sidebar li ul {
	float:left;
	margin:0;
	padding:8px 0 40px;
	width:100%;
}

#sidebar ul.help li {
	background-image:url("images/help.gif");
	background-position: left;
	background-repeat: no-repeat;
	padding-left:20px;
}

#sidebar li li {
	margin: 0;
	padding: 9px 0px;
	border-bottom: 1px solid #EBEBEB;
	width:100%;
	float:left;
	text-align:left;
}

#sidebar h2 {
	margin: 0px;
	padding: 4px 0px;
	border-bottom: 2px solid #6495ed;
	font: normal 16px Georgia, "Times New Roman", Times, serif;
	color: #454E55;
	background: #EBEBEB;
	float: left;
	width:100%;
	text-align: center;
}

#sidebar h3 {
	font-size: 77%;
	color: #454E55;
}

#sidebar p {
	margin: 0;
	line-height: normal;
	color: #D4C792;
}

#sidebar a {
	border: none;
	text-decoration: none;
}

#sidebar a:hover {
	text-decoration: underline;
}

#sidebar a.more-link{
	float:left;
	width:100%;
	text-align: right;
	padding: 4px 0px;
	border-top: 2px solid #6495ed;
	font: bold 12px Arial, "Times New Roman", Times, serif;
	font-style: italic;
	color: #454E55;
}

/* Footer */

#footer {
	width: 1000px;
	float:left;
	margin: 0px 0px 15px 0px;
	color: #757E82;
	background:#e8e8e8;
}

#footer p {
