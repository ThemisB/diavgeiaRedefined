        <rdfs:subPropertyOf rdf:resource="#participantOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#embodiedBy -->

    <owl:ObjectProperty rdf:about="#embodiedBy">
