 <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#language">
   <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
 </rdf:Description>

    <!-- http://www.metalex.eu/metalex/2008-05-02#agent -->

