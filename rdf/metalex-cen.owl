     -->

    


    <!-- http://www.metalex.eu/metalex/2008-05-02#countryCode -->

    <owl:DatatypeProperty rdf:about="#countryCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en"
            >Should be constrained to to or three characters from the ISO 3166-1 standard.</rdfs:comment>
        <rdfs:domain rdf:resource="#Country"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#issueID -->

    <owl:DatatypeProperty rdf:about="#issueID">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Issue"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#languageCode -->

    <owl:DatatypeProperty rdf:about="#languageCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Language"/>
        <rdfs:range rdf:resource="&xsd;language"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#manifestationTypeID -->

    <owl:DatatypeProperty rdf:about="#manifestationTypeID">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#ManifestationType"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#workTypeID -->

    <owl:DatatypeProperty rdf:about="#workTypeID">
