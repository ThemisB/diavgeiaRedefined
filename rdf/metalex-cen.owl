        <rdfs:subPropertyOf rdf:resource="#recordedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#realizes -->

    <owl:ObjectProperty rdf:about="#realizes">
