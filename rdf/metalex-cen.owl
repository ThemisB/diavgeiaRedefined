    </owl:ObjectProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#records -->

    <owl:ObjectProperty rdf:about="#records">
        <rdfs:range rdf:resource="#BibliographicObject"/>
        <rdfs:domain rdf:resource="#BibliographicObject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#refersTo -->

