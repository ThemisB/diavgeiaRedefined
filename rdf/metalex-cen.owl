        <rdfs:domain rdf:resource="#BibliographicExpression"/>
        <rdfs:range rdf:resource="#BibliographicWork"/>
        <rdfs:subPropertyOf rdf:resource="#records"/>
    </owl:ObjectProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#recipient -->

    <owl:ObjectProperty rdf:about="#recipient">
        <rdfs:domain rdf:resource="#Event"/>
        <rdfs:range rdf:resource="#Recipient"/>
        <rdfs:subPropertyOf rdf:resource="#participant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.metalex.eu/metalex/2008-05-02#recipientOf -->

    <owl:ObjectProperty rdf:about="#recipientOf">
        <rdfs:subPropertyOf rdf:resource="#participantOf"/>
        <owl:inverseOf rdf:resource="#recipient"/>
