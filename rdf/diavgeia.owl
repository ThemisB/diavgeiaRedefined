     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:ontology="http://data.europa.eu/eli/ontology#">
    <owl:Ontology rdf:about="http://diavgeia.gov.gr/ontology/">
        <owl:imports rdf:resource="http://data.europa.eu/eli/ontology#"/>
        <terms:contributor>Themis Beris</terms:contributor>
        <terms:creator>Themis Beris</terms:creator>
        <vann:preferredNamespacePrefix>dvg</vann:preferredNamespacePrefix>
        <terms:title>diavgeia.owl - An ontology describing decisions uploaded on Diavgeia</terms:title>
        <rdfs:comment>The &quot;diavgeia&quot; ontology describes decisions uploaded on Diavgeia&apos;s website (https://diavgeia.gov.gr/). The ontology links to other well-known greek ontologies (such as the greek legislation ontology (legislation.di.uoa.gr) and ELI (eli.fr))</rdfs:comment>
    </owl:Ontology>
    


