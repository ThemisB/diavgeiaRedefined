{
  "name": "n3-composer",
  "version": "1.0.0",
  "description": "App based on Nuxt & Vue that greek government institutions can use to compose their decisions in a HTML form and on backend generate the corresponding turtle files",
  "author": "Themis Beris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eellak/gsoc17-diavgeia/issues"
  },
  "homepage": "https://github.com/eellak/gsoc17-diavgeia/n3-composer#readme",
  "keywords": [
    "diavgeia",
    "n3-composer",
    "HTML",
    "turtle files"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eellak/gsoc17-diavgeia.git"
  },
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
